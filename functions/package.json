{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "serve": "firebase serve --only functions",
    "shell": "firebase functions:shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "watch": "npm-watch start",
    "_comment_start": "npm run shell",
    "start": "node --inspect=5000 node_modules/@google-cloud/functions-framework --target router --port 5000"
  },
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^1.2.1",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.4.2",
    "firebase-functions": "^3.13.0",
    "request": "^2.88.0"
  },
  "watch": {
    "start": "*.js"
  },
  "devDependencies": {
    "@jukben/emoji-search": "^2.0.1",
    "firebase-functions-test": "^0.1.6",
    "npm-watch": "^0.6.0"
  },
  "private": true
}
